<p id="notice"><%= notice %></p>

<h1>Rounds</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Weather</th>
      <th>Date</th>
      <th>Hole1</th>
      <th>Hole2</th>
      <th>Hole3</th>
      <th>Hole4</th>
      <th>Hole5</th>
      <th>Hole6</th>
      <th>Hole7</th>
      <th>Hole8</th>
      <th>Hole9</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.course %></td>
        <td><%= round.weather %></td>
        <td><%= round.date %></td>
        <td><%= round.hole1 %></td>
        <td><%= round.hole2 %></td>
        <td><%= round.hole3 %></td>
        <td><%= round.hole4 %></td>
        <td><%= round.hole5 %></td>
        <td><%= round.hole6 %></td>
        <td><%= round.hole7 %></td>
        <td><%= round.hole8 %></td>
        <td><%= round.hole9 %></td>
        <td><%= link_to 'Show', round %></td>
        <td><%= link_to 'Edit', edit_round_path(round) %></td>
        <td><%= link_to 'Destroy', round, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round', new_round_path %>
